{
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:prettier/recommended",
      "plugin:react-hooks/recommended"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "Promise": true
    },
    "rules": {
      "valid-jsdoc": 2
    }
  }